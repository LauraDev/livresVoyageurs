<IfModule mod_rewrite.c>
    Options -MultiViews
    RewriteEngine On
    RewriteBase /livresVoyageurs/public/
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)? index.php/$1 [QSA,L]
    RewriteRule ^/?$ /livresVoyageurs/public/ [R=301]
</IfModule>